{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "babel monorepo",
      "extends": ["monorepo:babel"],
      "matchUpdateTypes": ["digest", "patch", "minor", "major"],
      "matchPackagePatterns": ["^babel-plugin-", "@web-configs/babel-preset"],
      "automerge": true
    }
  ]
}
